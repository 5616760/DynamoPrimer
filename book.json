{
    "gitbook": "2.6.x",
    

    "plugins": [
    "-sharing",
	"toggle-chapters",
	"dynamo",
	"ga",
        {
            "name": "dynamo",
            "version": "git+https://github.com/DynamoDS/gitbook-plugin-dynamo.git"
    }
  ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-52186525-3"
        },
        "dynamo": {
            "customLinks": [
                {
                    "label": "Download",
                    "icon": "fa fa-file-pdf-o fa-lg",
                    "url": "Appendix/DynamoPrimer-Print.pdf"
        }],
            "languages": [
                {
                    "label": "English",
                    "code": "en",
                    "url": "http://dynamoprimer.com/en"
         },
                {
                    "label": "日本語",
                    "code": "ja",
                    "url": "http://dynamoprimer.com/ja"
         },
                {
                    "label": "Deutsch",
                    "code": "de",
                    "url": "http://dynamoprimer.com/de"
        }
      ],
            "currentLanguage": {
                "code": "en",
                "url": "http://dynamoprimer.com/en",
                "switchLangLabel": "Languages"
            },
            "favicon": "/images/icons/favicon.ico"
        }

    }
}
